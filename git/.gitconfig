[alias]
	lod = log --oneline --decorate
	undo = restore
	unstage = restore --staged
	last = log -1 HEAD
	co = checkout
	br = branch
	ci = commit
	st = status
	graph = log --oneline --decorate --graph
[core]
	editor = vim
[pull]
	rebase = false
[init]
	defaultbranch = main
[commit]
	gpgsign = true
[includeIf "gitdir:~/TravelX/"]
	path = ~/TravelX/.gitconfig
[includeIf "gitdir:~/Almundo/"]
	path = ~/Almundo/.gitconfig

